{
  "name": "ts-odata-v4-server",
  "version": "0.4.0",
  "description": "TS OData V4 Server",
  "main": "build/lib/index.js",
  "typings": "build/lib/index",
  "scripts": {
    "postinstall": "npm run build",
    "prebuild": "rimraf build",
    "build": "npm run tsc",
    "pretsc": "npm run lint",
    "tsc": "tsc",
    "prewatch": "rimraf build",
    "watch": "npm-watch",
    "es6": "copyfiles -u 1 src/**/*.js build",
    "lint": "tslint src/lib/**/*.ts -t verbose --force > .tslint",
    "typedoc": "rimraf docs && typedoc --name \"TS OData v4 Server\" --exclude \"**/?(utils|index).ts\" --excludeExternals --excludeNotExported --hideGenerator --excludePrivate --out docs src/lib",
    "release": "npm run build && npm publish",
    "release:beta": "npm run build && npm publish --tag beta"
  },
  "watch": {
    "tsc": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "quiet": false
    },
    "es6": {
      "patterns": [
        "src"
      ],
      "extensions": "js",
      "quiet": false
    }
  },
  "nyc": {
    "include": [
      "src/lib/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "build/**/*",
      "**/*.spec.ts",
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HaithamOumerzoug/ts-odata-v4-server.git"
  },
  "keywords": [
    "OData",
    "server",
    "V4",
    "server",
    "typescript"
  ],
  "author": "HaithamOUmerzoug",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HaithamOumerzoug/ts-odata-v4-server/issues"
  },
  "homepage": "https://github.com/HaithamOumerzoug/ts-odata-v4-server#readme",
  "dependencies": {
    "@types/body-parser": "1.17.0",
    "@types/cors": "2.8.4",
    "@types/express": "^4.16.0",
    "@types/qs": "^6.5.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "deepmerge": "^2.1.1",
    "express": "^4.16.3",
    "odata-v4-literal": "^0.1.1",
    "odata-v4-metadata": "^0.1.5",
    "odata-v4-parser": "^0.1.29",
    "odata-v4-service-document": "0.0.3",
    "odata-v4-service-metadata": "^0.1.6",
    "qs": "^6.5.2",
    "reflect-metadata": "^0.1.12",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/deepmerge": "^2.1.0",
    "@types/event-stream": "^3.3.34",
    "@types/jsonstream": "^0.8.30",
    "@types/lodash": "^4.14.112",
    "@types/mocha": "^5.2.5",
    "@types/mongodb": "^3.1.1",
    "@types/node": "^10.5.2",
    "@types/request": "^2.47.1",
    "@types/request-promise": "^4.1.42",
    "@types/stream-buffers": "^3.0.2",
    "JSONStream": "^1.3.3",
    "benchmark": "^2.1.4",
    "chai": "^4.1.2",
    "copyfiles": "^2.0.0",
    "event-stream": "^3.3.4",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.2",
    "mongodb": "^3.1.1",
    "mssql": "^4.1.0",
    "npm-watch": "^0.3.0",
    "nyc": "^12.0.2",
    "odata-v4-inmemory": "^0.1.9",
    "odata-v4-mongodb": "^0.1.12",
    "remap-istanbul": "^0.11.1",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.6",
    "stream-buffers": "^3.0.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typedoc": "^0.11.1",
    "typedoc-default-themes": "^0.5.0",
    "typedoc-plugin-external-module-name": "^1.1.1",
    "typescript": "^2.9.2",
    "xml-beautifier": "^0.4.0"
  }
}
